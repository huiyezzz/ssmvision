<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cm.hm.mapper.UserMapper">
    <select id="selectByName" parameterType="cm.hm.po.User" resultType="cm.hm.po.User">
        select * from sys_user where u_name=#{u_name} and u_password=#{u_password}
    </select>
</mapper>